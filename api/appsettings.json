{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Firebase": {
    "ProjectId": "your-project-id",
    "ApiKey": "your-api-key-here"
  },
  "R2": {
    "ACCOUNT_ID": "your-account-id",
    "ACCESS_KEY": "your-access-key-here",
    "SECRET_KEY": "your-secret-key-here "
  },
  "Jwt": {
    "Key": "your-secret-key-with-minimum-16-characters-here",
    "Issuer": "FoodHub-API",
    "Audience": "FoodHub-Client",
    "ExpiryInMinutes": 60
  }
}
